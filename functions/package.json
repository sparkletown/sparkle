{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "build": "tsc",
    "serve": "tsc -w | firebase emulators:start --only functions",
    "serve-debug": "tsc -w | firebase emulators:start --only functions --inspect-functions",
    "serve-hosting": "tsc -w | firebase emulators:start --only functions,hosting",
    "shell": "tsc -w | firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "eslint": "eslint --fix",
    "eslint:check": "eslint",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "npm run prettier && npm run eslint",
    "functions": "npm run lint && npm run build"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "agora-access-token": "^2.0.4",
    "date-fns": "^2.24.0",
    "firebase": "^8.8.0",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.0.0",
    "simple-oauth2": "^4.2.0",
    "twilio": "^3.63.0",
    "uuidv4": "^6.2.6",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@types/lodash": "^4.14.174",
    "@types/simple-oauth2": "^4.1.1",
    "@types/yup": "^0.29.3",
    "eslint": "^7.19.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.2.2",
    "prettier": "^2.2.1",
    "typescript": "^4.4.3"
  },
  "private": true
}
