    "twilio-video": "^2.19.0",
    "use-sound": "^4.0.1",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "ws": "^8.4.2",
    "xhr2": "^0.2.1",
    "yup": "^0.29.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
