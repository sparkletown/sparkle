{
  "name": "@sparkletown/sparkle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "init": "node scripts/init.js",
    "build": "craco build",
    "build:bundle-stats": "craco build --stats",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze:bundle-stats": "npx webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html",
    "analyze:bundle-stats:statoscope": "npx @statoscope/cli serve ./build/bundle-stats.json",
    "analyze:bundle-stats:duplicates-1": "npx inspectpack --action duplicates --stats build/bundle-stats.json",
    "analyze:bundle-stats:duplicates-2": "npx webpack-stats-duplicates ./build/bundle-stats.json",
    "lint": "npm run prettier:check && npm run eslint:check",
    "eslint": "eslint --fix",
    "eslint:check": "eslint src scripts functions/src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint src scripts functions/src --fix --ext .js,.jsx,.ts,.tsx",
    "prettier:write": "prettier --write \"{src,scripts,functions/src,configs,reporting}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:check": "prettier --check \"{src,scripts,functions/src,configs,reporting}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "reformat": "npm run eslint:fix && npm run prettier:write",
    "test": "npm run jest",
    "jest": "jest --passWithNoTests",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "firebase:emulate-all": "cd functions && npm run firebase:emulate-all",
    "firebase:emulate-functions": "cd functions && npm run firebase:emulate-functions",
    "firebase:emulate-functions-hosting": "cd functions && npm run firebase:emulate-functions-hosting",
    "firebase:emulate-functions-hosting:debug": "cd functions && npm run firebase:emulate-functions-hosting:debug",
    "browserslist:update-db": "npx browserslist@latest --update-db",
    "migrate": "fireway migrate --path=scripts/migration --require=\"ts-node/register\"",
    "migrate:dryrun": "fireway migrate --dryrun --path=scripts/migration --require=\"ts-node/register\"",
    "prune": "ts-prune > ts-prune.result.txt",
    "prepare": "husky install",
    "ncu:check": "npx npm-check-updates",
    "ncu:upgrade": "npx npm-check-updates --doctor --upgrade",
    "npm:save-audit": "npm audit > npm-audit.result.txt",
    "npm:generate-lockfile": "npm i --legacy-peer-deps --package-lock-only",
    "npm:install": "npm install --legacy-peer-deps",
    "npm:invalidate-cache": "npm cache clear --force"
  },
  "proxy": "http://localhost:3001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@headlessui/react": "^1.4.2",
    "@types/emoji-mart": "^3.0.9",
    "@types/faker": "^5.5.9",
    "@types/howler": "^2.2.4",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/scheduler": "^0.16.2",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.3",
    "babel-plugin-lodash": "^3.3.4",
    "csv-parse": "^5.0.4",
    "cypress": "^9.4.1",
    "cypress-file-upload": "^5.0.8",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "firebase-admin": "^10.0.2",
    "firebase-tools": "^10.1.4",
    "fireway": "^1.1.0",
    "gifwrap": "^0.9.2",
    "husky": "^7.0.4",
    "normalize.css": "^8.0.1",
    "npm-check-updates": "^12.2.1",
    "phin": "^3.6.1",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
